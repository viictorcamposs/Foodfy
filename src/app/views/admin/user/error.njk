{% extends '../layout.njk' %}

{% block content %}
    <div class="error-lottie"></div>

    <script src="/animation/lottie.min.js"></script>
    <script>
        const element = document.querySelector('.error-lottie')

        lottie.loadAnimation({
            container: element,
            renderer: 'svg',
            loop: false,
            autoplay: true,
            path: '/animation/blocked.json'
        })
    </script>
{% endblock %}